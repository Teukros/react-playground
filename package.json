{
  "name": "the-template",
  "version": "0.0.1",
  "description": "The only true project template",
  "scripts": {
    "build": "./node_modules/.bin/webpack -p",
    "dev": "./node_modules/.bin/webpack-dev-server -d --inline --content-base public/",
    "lint": "./node_modules/.bin/jshint --reporter=./node_modules/jshint-stylish src/ || true",
    "test": "./node_modules/.bin/mocha -r jsdom-global/register --reporter nyan --compilers js:babel-core/register src/**/*.test.js",
    "docs": "./node_modules/.bin/jsdoc src/ -r -d docs/ --verbose"
  },
  "author": "rbielawski",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "jsdoc": "^3.4.3",
    "jshint": "^2.9.4",
    "jshint-stylish": "^2.2.1",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "babel-preset-react": "^6.16.0",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2"
  }
}
